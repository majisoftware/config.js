{
  "name": "@maji/config.js",
  "version": "1.0.1",
  "main": "lib/Config.js",
  "scripts": {
    "lint": "standard",
    "test": "ava",
    "coverage": "nyc ava",
    "clean": "rm -rf .nyc_output coverage lib dist",
    "dist": "mkdir -p dist && browserify --standalone MajiConfig src/Config.js --outfile dist/config.js",
    "build": "babel src --out-dir lib"
  },
  "dependencies": {
    "debug": "^2.6.4",
    "isomorphic-fetch": "^2.2.1",
    "remove-trailing-slash": "^0.1.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.3.0",
    "delay": "^2.0.0",
    "nock": "^9.0.13",
    "nyc": "^10.2.0",
    "standard": "^10.0.2"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "html"
    ],
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "check-coverage": true
  }
}
